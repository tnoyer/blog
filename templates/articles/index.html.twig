{% extends 'base.html.twig' %}

{% block title %}Liste des articles{% endblock %}

{% block body %}
    <div class="container">
        <h1>Liste des articles</h1>
        {% for article in articles %}
            <h2>{{ article.titre }}</h2>
            <p>{{ article.contenu }}</p>
        {% endfor %}
    </div>
{% endblock %}
